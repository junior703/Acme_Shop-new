	<queryString>
		<![CDATA[SELECT c.id_compra, pr.producto, pr.pro as precio, dc.cantidad, dc.iva , dc.total
FROM compras c, proveedor p, productos pr, detalle_compra dc
where p.id_proveedor=c.id_proveedor
and c.id_compra = dc.id_compra
and pr.id_producto=dc.id_producto
